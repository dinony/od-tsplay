  "bugs": {
    "url": "https://github.com/dinony/od-tsplay/issues"
  },
  "homepage": "https://github.com/dinony/od-tsplay#readme",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "concurrently": "^3.5.0",
    "systemjs": "^0.20.19",
    "typescript": "^2.5.3"
  }
